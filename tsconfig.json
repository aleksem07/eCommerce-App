{
  "compilerOptions": {
    "target": "es2016",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "baseUrl": "./src",
    "paths": {
      "@Components/*": ["./components/*"],
      "@Pages/*": ["./pages/*"],
      "@Services/*": ["./services/*"],
      "@State/*": ["./state/*"],
      "@Utils/*": ["./utils/*"],
    }
  }
}
